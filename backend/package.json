{
  "name": "sistema-gestao-backend",
  "version": "1.0.0",
  "description": "Sistema de Gestão de Projetos e Serviços - Backend API",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "seed": "tsx src/database/seed.ts",
    "studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "worker": "tsx src/workers/index.ts",
    "generate": "prisma generate"
  },
  "keywords": [
    "project-management",
    "tasks",
    "services",
    "collaboration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "dotenv": "^16.0.3",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.7.0",
    "jsonwebtoken": "^9.0.0",
    "bcrypt": "^5.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-google-oauth20": "^2.0.0",
    "prisma": "^4.16.0",
    "@prisma/client": "^4.16.0",
    "redis": "^4.6.0",
    "bull": "^4.11.1",
    "ioredis": "^5.3.0",
    "express-validator": "^7.0.0",
    "sanitize-html": "^2.7.2",
    "csurf": "^1.11.0",
    "nodemailer": "^6.9.0",
    "firebase-admin": "^12.0.0",
    "pdfkit": "^0.13.0",
    "exceljs": "^4.3.0",
    "csv-parse": "^5.4.0",
    "uuid": "^9.0.0",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "winston": "^3.8.0",
    "joi": "^17.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.17",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.40.0",
    "prettier": "^2.8.8",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12",
    "nodemon": "^2.0.22",
    "tsx": "^3.14.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
